class Solution {
    public  int kthMissing(int[] arr, int k) {
        // code here
        int j=0;
        int c=0;
        int a=0;
        for(int i=1; ;i++){
           
            if(j<arr.length&&arr[j]==i){
                
                j++;
            }else{
                c++;
                if(c==k){
                    return i;
                }
            }
        }
        
    }
}
